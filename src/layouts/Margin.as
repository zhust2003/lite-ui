package layouts
{
    public class Margin
    {
        public var left:Number;
        public var right:Number;
        public var top:Number;
        public var bottom:Number;
        
        public function Margin(l:Number, r:Number, t:Number, b:Number) {
            left = l;
            right = r;
            top = t;
            bottom - b;
        }
    }
}